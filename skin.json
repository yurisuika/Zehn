{
  "Conditions": {
    "Navbar: Display Announcements Button": {
      "description": "Toggles displaying the announcements button on the navbar.",
      "default": "no",
      "values": {
        "no": {
          "TargetCss": {
            "affects": [".*"],
            "src": "css/config/navbar/announcements/hide.css"
          }
        },
        "yes": {
          "TargetCss": {
            "affects": [".*"],
            "src": "css/config/navbar/announcements/show.css"
          }
        }
      }
    },
    "Navbar: Display Add Game Button": {
