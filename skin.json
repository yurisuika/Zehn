{
  "Conditions": {
    "Color Mode": {
      "description": "Choose between dark mode or light mode.",
      "tab": "Theme",
      "default": "Dark",
      "values": {
        "Dark": {
          "TargetCss": {
            "affects": [".*"],
            "src": "config/theme/mode/dark/theme.css"
          },
          "TargetJs": {
            "affects": [".*"],
            "src": "config/theme/mode/dark/theme.js"
          }
        },
        "Light": {
          "TargetCss": {
            "affects": [".*"],
            "src": "config/theme/mode/light/theme.css"
          },
          "TargetJs": {
            "affects": [".*"],
            "src": "config/theme/mode/light/theme.js"
          }
        }
      }
    },
    "Automatic Mode": {
      "description": "Automatically sets the color mode based on your system setting. Overrides the Color Mode setting.",
      "tab": "Theme",
      "default": "no",
      "values": {
        "no": {},
        "yes": {
          "TargetCss": {
            "affects": [".*"],
            "src": "config/theme/auto/enable.css"
          }
        }
      }
    },
    "Background Color Blend": {
      "description": "Blends a color into the background. This includes window backgrounds and acrylic effects. You may select this color from the Colors tab.",
      "tab": "Theme",
      "default": "no",
      "values": {
        "no": {},
        "yes": {
          "TargetCss": {
            "affects": [".*"],
            "src": "config/theme/blend/color/background/enable.css"
          }
        }
      }
    },
    "Foreground Color Blend": {
      "description": "Blends a color into the foreground. This includes text, buttons, and other foreground elements. You may select this color from the Colors tab.",
      "tab": "Theme",
      "default": "no",
      "values": {
        "no": {},
        "yes": {
          "TargetCss": {
            "affects": [".*"],
            "src": "config/theme/blend/color/foreground/enable.css"
          }
        }
      }
    },
    "Color Blend Amount": {
      "description": "Sets how much color is blended into the theme. Lower values produce a better blend contrast, but higher values produce a truer color.",
      "tab": "Theme",
      "default": "50%",
      "values": {
        "0%": {
          "TargetCss": {
            "affects": [".*"],
            "src": "config/theme/blend/amount/0%.css"
          }
        },
        "25%": {
          "TargetCss": {
            "affects": [".*"],
            "src": "config/theme/blend/amount/25%.css"
          }
        },
        "50%": {
          "TargetCss": {
            "affects": [".*"],
            "src": "config/theme/blend/amount/50%.css"
          }
        },
        "75%": {
          "TargetCss": {
            "affects": [".*"],
            "src": "config/theme/blend/amount/75%.css"
          }
        },
        "100%": {
          "TargetCss": {
            "affects": [".*"],
            "src": "config/theme/blend/amount/100%.css"
          }
        }
      }
    },
